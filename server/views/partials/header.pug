header(class="header")
    div(class="header__content")
        a(href="/" class="header__logo")
            img(src="/images/IconText.svg" class="logo") 
        div(class="catalogue__popup") ыыы
        div(class="header__tabs")
            each i in links
                if i.src == "/catalogue"
                    a(href=i.src class="tab header__tab") #{i.name}
                else 
                    a(href=i.src class="tab header__tab") #{i.name}
            div(class="icons")
                a(href="/profile" class="tab header__tab")
                    svg.icon(xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor")
